
section.mm-clock
  .column
    time.mm-time
      span.mm-hour 09
      span.mm-clock-divider :
      span.mm-minute 18
    footer
      div.mm-date SÃ¶ndag, September 17
      div.mm-names 
        span #{todaysNames}

script.
  var socket = io();

  socket.on('clock', function(data) {
    document.querySelector('.mm-hour').innerHTML = data.hour;
    document.querySelector('.mm-minute').innerHTML = data.minute;
    document.querySelector('.mm-date').innerHTML = data.date;
  });

  socket.on('todaysNames', function(data) {
    console.log('getting names', data);
  });